public class Solution
{
    public static Node findMiddle(Node head)
    {
        int count=0;
        Node temp=head;
        while(temp!=null){
            count++;
            temp=temp.next;
        }
        // System.out.print(count);
        int mid=count/2;
        // System.out.print(mid);
        Node r=head;
        int i=0;
        while(i<mid){
            r=r.next;
            i++;

        }
        return r;
        // Write your code here.
    }
}
